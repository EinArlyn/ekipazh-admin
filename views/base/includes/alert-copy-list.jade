.alert-copy-list-popup
  span.pop-up-header
    | 
  .alert-name-pop-up
    =i18n.__('Select the set to copy its properties and composition')
  .search-filter 
    .search-filter-name
      span
        =i18n.__('Filter')
      span
        |(
      span
        =i18n.__('Set name')
      span
        |)
    input(class="search-input-copy-list" id="search-input-copy-list" type="text" value="")
  select#element-type(class="select-copy-type") 
  .btns-align
    input.pop-up-submit-btn(id="submit-copy", href="#", type="button" value=i18n.__('Copy'))
    input.pop-up-deny-btn(id="deny-remove", href="#", type="button" value=i18n.__('Cancel'))