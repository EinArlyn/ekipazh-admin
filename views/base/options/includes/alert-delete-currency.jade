.pop-up-default.delete-alert.delete-currency-alert
  .pop-up-close-wrap.pop-up-default-close
    a.pop-up-close(value="" href="#")
  span.pop-up-header
    =i18n.__('Removing currency')
  .alert-name-pop-up
    =i18n.__('Are you sure?') 
  .btns-align
    input.pop-up-submit-btn(id="delete-currency-submit", data-currency="" href="#", type="button" value=i18n.__('Remove'))
    input.pop-up-deny-btn(id="delete-currency-deny", href="#", type="button" value=i18n.__('Cancel'))