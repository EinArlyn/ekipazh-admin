.pop-up-default.alert-send-order
  .pop-up-close-wrap
    a(class="pop-up-close", href="#")
  span.pop-up-header
    =i18n.__('Confirm action')
  .row
    .col-md-12.text-center#order-action-type
      =i18n.__('Send order to factory for invoicing?')
  .row
    .col-md-6.text-center
      input.pop-up-submit-btn#send-order-btn(type="button" value=i18n.__('Yes'))
    .col-md-6.text-center
      input.pop-up-deny-btn(type="button" value=i18n.__('No'))

      
